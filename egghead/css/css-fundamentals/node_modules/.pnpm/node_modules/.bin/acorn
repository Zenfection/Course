#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/acorn@7.4.1/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/acorn@7.4.1/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
