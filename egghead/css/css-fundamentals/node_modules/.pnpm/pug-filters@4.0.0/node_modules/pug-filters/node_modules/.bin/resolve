#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/bin/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/resolve@1.22.2/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/bin/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/resolve@1.22.2/node_modules:/Volumes/Data/Github/Course/egghead/css/css-fundamentals/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.2/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.2/node_modules/resolve/bin/resolve" "$@"
fi
